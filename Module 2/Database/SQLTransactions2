SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
BEGIN TRAN;
SELECT *
FROM Students
WHERE StudentId = 1;
COMMIT TRAN;

---------
SET TRANSACTION ISOLATION LEVEL READ COMMITTED;
BEGIN TRANSACTION;
UPDATE 
   Students  
SET 
    Lastname = 'Smith', 
    Firstname = 'Sam' 
WHERE 
    StudentId = 2

COMMIT TRANSACTION;

---------
SET TRANSACTION ISOLATION LEVEL READ COMMITTED;
BEGIN TRANSACTION;

INSERT INTO Students
VALUES('Yasir', 'Jordan', '094494985', 'duri.cu@pellentesquetellussem.org', 3, '1975-07-07', '2021-01-27', 'QCY92XUE7QT', 3)

COMMIT TRANSACTION;